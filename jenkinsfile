pipeline {
    agent any

    stages {
		stage('Build') {
            steps{
                echo "building"
            }
        }
	    stage('Run Tests') {
	    	parallel {
		    	stage('Test On Windows') {
				    steps {
    					echo "windows"
				    }
			    }
			    stage('Test On Linux') {
    				steps {
					    echo "Linux"
				    }
			    }
		    }
        }
    }
}